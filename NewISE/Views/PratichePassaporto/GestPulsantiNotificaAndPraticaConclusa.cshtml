@model NewISE.Models.ViewModel.GestPulsantiPassaportoModel



<div class="row">
    <div class="col-xs-5 col-xs-offset-7" style="margin-top: 10px; margin-bottom: 10px;">

        @if (Model.praticaConclusa == false)
        {
            if (Model.notificaRichiesta == false)
            {
                if (Model.esistonoRichiesteAttive == false)
                {
                    if (Model.esistonoRichiesteSalvate == true)
                    {
                        <button class="btn btn-default btn-sm " id="btNotificaRichiestaPassaporti" onclick="NotificaRichiestaPassaporto();">
                            <i aria-hidden="true" class="fa fa-2x fa-envelope"></i>
                            Notifica richiesta
                        </button>

                        <button class="btn btn-default btn-sm disabled" disabled id="btPraticaConclusaPassaporti">
                            <i aria-hidden="true" class="fa fa-2x fa-envelope"></i>
                            Pratica conclusa
                        </button>

                    }
                    else
                    {
                        <button class="btn btn-default btn-sm disabled" disabled id="btNotificaRichiestaPassaporti">
                            <i aria-hidden="true" class="fa fa-2x fa-envelope"></i>
                            Notifica richiesta
                        </button>


                        <button class="btn btn-default btn-sm disabled" disabled id="btPraticaConclusaPassaporti">
                            <i aria-hidden="true" class="fa fa-2x fa-envelope"></i>
                            Pratica conclusa
                        </button>
                    }
                }
                else
                {
                    <button class="btn btn-default btn-sm disabled" disabled id="btNotificaRichiestaPassaporti">
                        <i aria-hidden="true" class="fa fa-2x fa-envelope"></i>
                        Notifica richiesta
                    </button>

                    <button class="btn btn-default btn-sm disabled" disabled id="btPraticaConclusaPassaporti">
                        <i aria-hidden="true" class="fa fa-2x fa-envelope"></i>
                        Pratica conclusa
                    </button>

                }
            }
            else
            {
                <button class="btn btn-default btn-sm disabled" disabled id="btNotificaRichiestaPassaporti">
                    <i aria-hidden="true" class="fa fa-2x fa-envelope"></i>
                    Notifica richiesta
                </button>

                <button class="btn btn-default btn-sm " id="btPraticaConclusaPassaporti" onclick="">
                    <i aria-hidden="true" class="fa fa-2x fa-envelope"></i>
                    Pratica conclusa
                </button>

            }
        }
        else
        {
            <button class="btn btn-default btn-sm disabled" disabled id="btNotificaRichiestaPassaporti">
                <i aria-hidden="true" class="fa fa-2x fa-envelope"></i>
                Notifica richiesta
            </button>

            <button class="btn btn-default btn-sm disabled" disabled id="btPraticaConclusaPassaporti">
                <i aria-hidden="true" class="fa fa-2x fa-envelope"></i>
                Pratica conclusa
            </button>
        }


    </div>
</div>
