@model NewISE.Models.ViewModel.GestioneChkincludiPassaportoModel

@if (Model.esisteDoc || Model.disabilitaChk)
{

    @Html.CheckBoxFor(model => model.includiPassaporto, new { @onclick = "return null;", @disabled = "disabled" })

}
else
{

    @Html.CheckBoxFor(model => model.includiPassaporto, new { @onclick = "ConfermaEscludiPassaporto(this," + @Model.idFamiliare + ", " + Convert.ToDecimal(Model.parentela) + ");" })

}
