@using NewISE.Models.DBModel.Enum
@model NewISE.Models.ViewModel.GestioneChkincludiPassaportoModel


@if (Model.esisteDoc == false || Model.disabilitaChk == true )
{

    @Html.CheckBoxFor(model => model.includiPassaporto, new { @onclick = "return null;", @disabled = "disabled" })

}
else
{

    @Html.CheckBoxFor(model => model.includiPassaporto, new { @onclick = "ConfermaIncludiEscludiPassaporto(this," + @Model.idFamiliare + ", " + Convert.ToDecimal(Model.parentela) + ");" })

}
