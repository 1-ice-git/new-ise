@using NewISE.Models.DBModel.Enum
@using NewISE.Models.DBModel

@model NewISE.Models.ViewModel.GestioneChkincludiPassaportoModel


@if (Model.esisteDoc == false || Model.disabilitaChk == true || Model.parentela==EnumParentela.Richiedente )
{

    @Html.CheckBoxFor(model => model.includiPassaporto, new { @onclick = "return null;", @disabled = "disabled" })

}
else
{

    @Html.CheckBoxFor(model => model.includiPassaporto, new { @onclick = "ConfermaIncludiEscludiPassaporto_var(this," + @Model.idFamiliare + ", " + Convert.ToDecimal(Model.parentela) + ");" })

}
