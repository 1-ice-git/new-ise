        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.coan, htmlAttributes: new { @class = "control-label col-md-2" })<span class="text-danger">*</span>
        <div class="col-xs-10">
            <input id="dataPartenza" name="dataPartenza" type="datetime" value="" class="form-control" />
