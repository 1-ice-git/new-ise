        <div class="col-xs-10">
            <input id="dataPartenza" name="dataPartenza" type="datetime" value="" class="form-control" />
        </div>
        @Html.ValidationMessageFor(model => model.dataPartenza, "", new { @class = "text-danger" })
    </div>
    <div class="form-group">
